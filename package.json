{
  "name": "@guardian/actions-merge-release-changes-to-protected-branch",
  "version": "1.2.0",
  "description": "A GitHub action to open, approve and merge a PR of changes generated during a release on a protected branch",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --detectOpenHandles",
    "build": "ncc build src/index.ts -o dist --license licenses.txt",
    "prettier:check": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "lint": "eslint . --ext .ts",
    "prepare": "husky install"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0"
  },
  "devDependencies": {
    "@guardian/eslint-config-typescript": "^9.0.0",
    "@guardian/prettier": "^7.0.0",
    "@octokit/types": "^12.4.0",
    "@octokit/webhooks-types": "^7.3.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.7",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "type-fest": "^4.9.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "yarn": "please-use-npm"
  }
}
