{
  "name": "@guardian/actions-merge-release-changes-to-protected-branch",
  "version": "1.0.0",
  "description": "A GitHub action to open, approve and merge a PR of changes generated during a release on a protected branch",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --detectOpenHandles",
    "build": "ncc build src/index.ts -o dist --license licenses.txt",
    "prettier:check": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "lint": "eslint . --ext .ts",
    "prepare": "husky install"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@actions/core": "^1.3.0",
    "@actions/exec": "^1.1.0",
    "@actions/github": "^5.0.0"
  },
  "devDependencies": {
    "@guardian/eslint-config-typescript": "^0.6.0",
    "@guardian/prettier": "^0.6.0",
    "@octokit/types": "^6.19.0",
    "@octokit/webhooks-types": "^4.1.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.1",
    "@vercel/ncc": "^0.28.5",
    "eslint": "^7.29.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "husky": "^7.0.1",
    "jest": "^26.6.3",
    "prettier": "^2.3.2",
    "ts-jest": "^26.5.6",
    "type-fest": "^1.2.2",
    "typescript": "^4.3.5"
  },
  "engines": {
    "yarn": "please-use-npm"
  }
}
